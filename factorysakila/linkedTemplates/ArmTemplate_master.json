						"value": "[parameters('sakila_DW_simple_properties_typeProperties_server')]"
					},
					"sakila_DW_simple_properties_typeProperties_database": {
						"value": "[parameters('sakila_DW_simple_properties_typeProperties_database')]"
					},
					"sakila_DW_simple_properties_typeProperties_userName": {
						"value": "[parameters('sakila_DW_simple_properties_typeProperties_userName')]"
					},
					"sakila_staging_properties_typeProperties_server": {
						"value": "[parameters('sakila_staging_properties_typeProperties_server')]"
